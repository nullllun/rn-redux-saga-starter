{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "rules": {
    "import/no-unresolved": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0
  },
  "globals": {
    "__DEV__": true
  },
  "env": {
    "mocha": true
  }
}
